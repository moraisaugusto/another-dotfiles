[colors]
background = #222
background-alt = #444
foreground = #dfdfdf
foreground-alt = #55
primary = #ffb52a
secondary = #e60053
alert = #bd2c40

[global/wm]
; used for bottom aligned bars
;margin-top = 5
; used for top aligned bars
;margin-bottom = 5

[bar/top]
; Environment variables can be referenced using:
;key = ${env:VAR:fallback value}
monitor = ${env:MONITOR:HDMI-1-2}
;monitor = eDP-1-1
width = 100%
height = 18
offset-x = 0
offset-y = 0

;background = ${xrdb:color9}
background = ${colors.background}
foreground = ${colors.foreground}

overline-size = 2
overline-color = #f00
underline-size = 2
underline-color = #00f

border-bottom = 2
border-bottom-color = #333

spacing = 1
padding-left = 0
padding-right = 2
module-margin-left = 1
module-margin-right = 2

;font-0 = fixed:pixelsize=10;0
;font-1 = unifont:size=6:heavy:fontformat=truetype;-2
;font-3 = FontAwesome:size=7;-1

font-0 = NotoSans-Regular:size=8;0
font-1 = FontAwesome:size=8;-2
font-2 = Unifont:size=6;-3
font-3 = Source Code Pro Regular:size=8;0
; font-2 = siji:pixelsize=10;0
; font-3 = NotoSans-Regular:size=8;-1
;font-4 = MaterialIcons:size=10;0

;modules-left = bspwm xwindow
;modules-left = powermenu mpd
;modules-center = bspwm xwindow
;modules-right = mpd backlight volume xkeyboard memory cpu wlan battery temperature date powermenu
;modules-right = xbacklight volume xkeyboard wireless-network battery memory cpu wlan date

modules-left = 
modules-center = bspwm xwindow
; xbacklight currently not working
modules-right = volume wireless-network battery date

tray-position = right
tray-padding = 2
;tray-transparent = true
;tray-background = #0063ff

;wm-restack = bspwm
;wm-restack = i3

;override-redirect = true

;scroll-up = bspwm-desknext
;scroll-down = bspwm-deskprev

[module/xbacklight]
type = internal/xbacklight
; ls -1 /sys/class/backlight/
card = intel_backlight
output = eDP-1-1
;
; Available tags:
;   <label> (default)
;   <bar>
format = %{A4:backlight_percentage%__p5:}%{A5:backlight_percentage%__m5:} <ramp> <bar> %{A}%{A}
; Available tokens:
;   %percentage% (default)
;label = %percentage%

; Required if <ramp> is used
; ramp-0 = ğŸŒ•
; ramp-1 = ğŸŒ”
; ramp-2 = ğŸŒ“
; ramp-3 = ğŸŒ’
; ramp-4 = ğŸŒ‘


; Required if <bar> is used
bar-width = 10
bar-format = %{+u}%{+o}%fill%%{-u}%{-o}%indicator%%{+u}%{+o}%empty%%{-u}%{-o}
bar-indicator = |
bar-indicator-foreground = #ddffffff
bar-indicator-font = 3
bar-fill = â–ˆ
bar-fill-foreground = #99ffffff
bar-fill-font = 3
bar-empty = â–ˆ
bar-empty-font = 3
bar-empty-foreground = #44ffffff

;format = <label> <bar>
;label = BL

;bar-width = 10
;bar-indicator = â”‚
;bar-indicator-foreground = #ff
;bar-indicator-font = 2
;bar-fill = â”€
;bar-fill-font = 2
;bar-fill-foreground = #9f78e1
;bar-empty = â”€
;bar-empty-font = 2
;bar-empty-foreground = ${colors.foreground-alt}

[module/volume]
type = internal/volume
; Use the following command to list available mixer controls:
; $ amixer scontrols | sed -nr "s/.*'([[-alnum-]]+)'.*/\1/p"
;speaker-mixer = Speaker
;headphone-mixer = Headphone
; Use the following command to list available device controls
; $ amixer controls | sed -r "/CARD/\!d; s/.*=([0-9]+).*name='([^']+)'.*/printf '%3.0f- %s\n' '\1' '\2'/e" | sort
;headphone-id = 13
; Use volume mapping (similar to amixer -M and alsamixer), where the increase in volume is linear to the ear
; Default: false
mapping = true

format-volume = <label-volume> <bar-volume>
label-volume = VOL
label-volume-foreground = ${root.foreground}

;format-muted-prefix = "îˆ‚ "
format-muted-foreground = ${colors.foreground-alt}
label-muted = sound muted

bar-volume-width = 10
bar-volume-foreground-0 = #55aa55
bar-volume-foreground-1 = #55aa55
bar-volume-foreground-2 = #55aa55
bar-volume-foreground-3 = #55aa55
bar-volume-foreground-4 = #55aa55
bar-volume-foreground-5 = #f5a70a
bar-volume-foreground-6 = #ff5555
bar-volume-gradient = false
bar-volume-indicator = â”‚
bar-volume-indicator-font = 2
bar-volume-indicator-foreground = #ff
bar-volume-fill = â”€
bar-volume-fill-font = 2
bar-volume-empty = â”€
bar-volume-empty-font = 2
bar-volume-empty-foreground = ${colors.foreground-alt}

